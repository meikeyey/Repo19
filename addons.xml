<?xml version="1.0" encoding="UTF-8"?>

<addons>

<!-- 1- Repositories A -->

<!-- 1.1- Elementum Unofficial Repository -->
<addon id="Elementum.Unofficial.Repository" name="Elementum Unofficial Repository" version="1.0.2" provider-name="Elementum Unofficial">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="Elementum Unofficial Repository">
        <dir>
            <info compressed="false">https://rock.seedhost.eu/kcat123/Repo/zips3/addons.xml</info>
            <checksum>https://rock.seedhost.eu/kcat123/Repo/zips3/addons.xml.md5</checksum>
            <datadir zip="true">https://rock.seedhost.eu/kcat123/Repo/zips3/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en"></summary>
        <description lang="en"></description>
        <disclaimer lang="en"></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <news></news>
    </extension>
</addon>

<!-- 1.2-Lanik (Python 3) Repository -->
<addon id="lanik_p3.repo" name="Lanik (Python 3) Repository" provider-name="Dalibor Lanik" version="1.0.0">
        <requires>
            <import addon="xbmc.addon" version="12.0.0" />
        </requires>
        <extension name="Lanik (Python 3) Repository" point="xbmc.addon.repository">
            <info compressed="false">http://kodi.lanik.org/_p3repo/addons.xml</info>
            <checksum>http://kodi.lanik.org/_p3repo/addons.xml.md5</checksum>
            <datadir zip="true">http://kodi.lanik.org/_p3repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Repository for my KODI plugins</summary>
            <description>KODI Repository for stuff made by D. Lanik</description>
            <platform>all</platform>
    </extension>
</addon>

</addons>
